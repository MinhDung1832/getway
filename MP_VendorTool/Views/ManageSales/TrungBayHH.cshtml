@using MP_VendorTool.Common
@using MP_VendorTool.Models.Order
@{
    ViewBag.Title = "Quản lý trưng bày ngành hàng";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content-wrapper">
    <div class="page-content fade-in-up">
        <div class="ibox">
            <div class="ibox-body">
                <div class="content_search">
                    <div class="AutoFix">
                        <div class="fromsearch">
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="box_with">
                                        <div class="form-group row">
                                            <label for="vendorName" class="col-sm-4 col-form-label">Nhà cung cấp</label>
                                            <div class="col-sm-8">
                                                @if (Session["VendorName"] != null && Session["VendorName"].ToString().Length > 0)
                                                {
                                                    <input type="text" class="form-control" id="NhaCCFull" name="NhaCCFull" value="@Session["VendorCode"] - @Session["VendorName"]" disabled>
                                                    <input type="hidden" class="form-control" id="NhaCC" name="NhaCC" value="@Session["VendorCode"]" disabled>
                                                }
                                                else
                                                {
                                                    <input type="hidden" class="form-control" id="NhaCCFull" name="NhaCCFull" disabled>
                                                    <select id="NhaCC" class="form-control selectpicker" data-live-search="true" onchange="ShowNCCFull(); js_onchange_NCC_MaHang(); js_searchbox()">
                                                        @if (ViewBag.list_vendor != null)
                                                        {
                                                            <option value="">--- Chọn NCC ---</option>
                                                            foreach (MP_VendorTool.Models.Order.objCombox box in (List<MP_VendorTool.Models.Order.objCombox>)ViewBag.list_vendor)
                                                            {
                                                                <option value="@box.Code">@box.Code - @box.Name</option>
                                                            }
                                                        }
                                                    </select>
                                                }
                                            </div>
                                        </div>

                                        <div class="form-group row">
                                            <label for="textMien" class="col-sm-4 col-form-label">Mã hàng</label>
                                            <div class="col-sm-8">
                                                <select id="MaHang" class="form-control selectpicker" data-live-search="true" onchange="js_searchbox()">
                                                    <option value="">--- Chọn mã hàng ---</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-1"></div>
                                <div class="col-md-4">
                                    <div class="box_with">
                                        <div class="form-group row">
                                            <label for="Ngay" class="col-sm-4 col-form-label">Trạng thái</label>
                                            <div class="col-sm-8">
                                                <select id="TrangThai" class="form-control selectpicker" data-live-search="true" onchange="js_searchbox()">
                                                    <option value=""> -- Chọn trạng thái -- </option>
                                                    <option value="NEW">Gửi duyệt</option>
                                                    <option value="APPROVED">Đã duyệt</option>
                                                    <option value="REJECT">Từ chối</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label for="textStatus" class="col-sm-4 col-form-label">Hiệu quả</label>
                                            <div class="col-sm-8">
                                                <select id="XucTien" class="form-control selectpicker" data-live-search="true" onchange="js_searchbox()">
                                                    <option value=""> -- Chọn đánh giá -- </option>
                                                    <option value="1">Mở rộng</option>
                                                    <option value="0">Thu hẹp</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3" style="text-align: center;">
                                    <div class="form-group row">
                                        <div class="col-sm-12">
                                            <a href="#" class="btn btn-secondary px-5 btgetw" onclick="js_searchbox()"><i class="fa fa-search" aria-hidden="true" style="margin-top: 3px; margin-right: 3px;"></i> Tìm kiếm</a>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-sm-12">
                                            <a href="#" class="btn btn-secondary px-5 btgetw" onclick="js_nextPage()"><i class="fa fa-newspaper-o" aria-hidden="true" style="margin-top: 3px; margin-right: 3px;"></i> Mở rộng KD</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="div-content" style=" padding-top: 83px; ">
                    @Html.Partial("~/Views/ManageSales/Partial/__TrungBayHH.cshtml")
                </div>
            </div>
        </div>
    </div>
</div>


<style>
    .form-text {
        display: inline-table;
        margin-top: 0.25rem;
        width: 98% !important;
    }

    #tbl_ItemTang_length {
        position: absolute;
        bottom: -7px;
        left: 206px;
    }

    div#tbl_Item_MRKD_length {
        display: none
    }

    div#tbl_Item_MRKD_paginate {
        display: none
    }
</style>

<div class="modal" id="popupMoRongKD" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document" style="width: 60% !important; max-width: 60% !important; border-radius: 0; ">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Mở rộng kinh doanh</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body" style=" width: 100%; margin: 10px;">
                <div class="box_ItemOrder">
                    <div class="autofixx" style="">
                        <div class="col-md-6 ">
                            <div class="form-group row">
                                <label for="MaCTKM" class="col-sm-3 col-form-label">Mã hàng</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="MaHangPop" name="MaHangPop" disabled>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="MaCTKM" class="col-sm-3 col-form-label">Cửa hàng</label>
                                <div class="col-sm-9">
                                    <select id="CuaHang" class="form-control selectpicker" data-live-search="true" onchange="js_searchbox_P()">
                                        <option value="">--- Chọn của hàng ---</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6 ">
                            <div class="form-group row">
                                <label for="MaCTKM" class="col-sm-3 col-form-label">Miền</label>
                                <div class="col-sm-9">
                                    <select id="Mien" class="form-control selectpicker" data-live-search="true" onchange="js_searchbox_P()">
                                        <option value=""> -- Chọn vùng miền -- </option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="MaCTKM" class="col-sm-3 col-form-label">Tỉnh thành</label>
                                <div class="col-sm-9">
                                    <select id="TinhThanh" class="form-control selectpicker" data-live-search="true" onchange="js_searchbox_P()">
                                        <option value=""> -- Chọn tỉnh thành -- </option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div style=" clear:both"></div>
                    <div class="row">
                        <div id="div-content_MRKD" style="width: 98%; ">
                            @Html.Partial("~/Views/ManageSales/Partial/__MoRongKD.cshtml")
                        </div>
                    </div>
                    <div style="margin:10px">
                        <div style="text-align: center;">
                            <a class="form-control btn btn-info" onclick="js_saveCheck()" style="width:100px; margin-top:10px; color:#fff">
                                Lưu lại
                            </a>
                            <a data-dismiss="modal" class="form-control btn btn-info" style="width: 100px; margin-top: 10px; color: #fff; background:#b1b1b1 ">
                                <i class="fa fa-times" aria-hidden="true" style="float:left; margin-left: 6px;margin-top: 3px;"></i>Đóng
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


