<script>
    $(function () {
        $('#txtHead').html("Quản lý trưng bày ngành hàng");
        $('#f_dev').html("");
        $('#f_header').html("");
        $('#v_review').hide();
        $('#v_view').hide();
        $('#v_approve').hide();
        js_GetList();
        js_onchange_NCC_MaHang();

    });
    function js_searchbox() {
        js_GetList();
    }
    function ShowNCCFull() {
        $("#NhaCCFull").val($('#NhaCC').find(":selected").text())
    }
    function js_ReloadTable() {
        imgrecall();
        $('#tbl_Item').DataTable({
            lengthMenu: [[200, 50, 70, 100, -1], [200, 50, 70, 100]],
            //"order": [[0, "desc"]],
            //columnDefs: [
            //    { 'targets': 7, 'orderable': false },
            //    { 'targets': 8, 'orderable': false },
            //    { 'targets': 9, 'orderable': false }
            //],
            "searching": false,
            fixedHeader: true,
            fixedHeader: {
                headerOffset: 184
            },
            autoWidth: false,
            "language": {
                "emptyTable": "Không có dữ liệu !"
            }
        });
    }

    function imgrecall() {
        tb_init('a.thickbox, area.thickbox, input.thickbox');
        $('img.content').hover(function () {
            $(this).addClass('transition');
        }, function () {
            $(this).removeClass('transition');
        });
    }
    function js_GetList() {
        var vendorNo = fomart_split($("#NhaCCFull").val(), 0);
        var MaHang = fomart_split($("#MaHang").val(), 0);
        var TrangThai = $("#TrangThai").val();
        var XucTien = $("#XucTien").val();
        js_Loading(true, 1);
        $.ajax({
            url: '/ManageSales/GetList_TrungBayHH',//
            type: 'POST',
            data: JSON.stringify({ vendorNo: vendorNo, MaHang: MaHang, TrangThai: TrangThai, XucTien: XucTien }),
            contentType: 'application/json, charset=utf-8',
            beforeSend: function () {
                js_Loading(true, 1);
            },
            success: function (data) {
                $("#div-content").find('tbody').empty();
                $('#div-content').html(data);
                js_ReloadTable();
                js_Loading(false, 1);
            },
            error: function () {
                js_Loading(false, 1);
            }
        });
    }

    function js_onchange_NCC_MaHang() {
        var NCC = fomart_split($("#NhaCCFull").val(), 0);
        $('#MaHang').empty();
        if (NCC.length > 0) {
            // Call MaHang
            $.ajax({
                url: '/ManageSales/GetList_NCC_Produts',
                type: 'POST',
                data: JSON.stringify({ NCC: NCC }),
                contentType: 'application/json, charset=utf-8',
                beforeSend: function () {
                    js_Loading(true, 1);
                },
                success: function (data) {
                    var row = JSON.parse(data);
                    if (row != null) {
                        $('#MaHang').empty();
                        $('#MaHang').append('<option value="">--Chọn mã hàng--</option>');
                        for (i in row) {
                            $('#MaHang').append('<option value="' + row[i].Code + ' - ' + row[i].Name + '">' + row[i].Code + ' - ' + row[i].Name + '</option>');
                        }
                    }
                    $('#MaHang').selectpicker('refresh');
                    js_Loading(false, 1);
                },
                error: function () {
                    js_Loading(false, 1);
                }
            });
        }
    }



    function js_nextPage() {
        var cboxes = document.getElementsByName('check[]');
        var len = cboxes.length;
        var arrCheckbox = [];
        var strCheckbox = '';
        for (var i = 0; i < len; i++) {
            if (cboxes[i].checked) {
                arrCheckbox.push(cboxes[i].value);
                strCheckbox += cboxes[i].value;
            }
        }
        console.log(strCheckbox);
        var Mahang = fomart_split1(strCheckbox, 0);
        var Tenhang = fomart_split1(strCheckbox, 1);
        var MaNCC = fomart_split1(strCheckbox, 2);
        if (arrCheckbox.length > 0) {
            $('#MaHangPop').val(Mahang + ' - ' + Tenhang);
            js_showpopup(Mahang, MaNCC);
        } else {
            alert("Bạn phải chọn mã hàng");
        }
    }
    function fomart_split1(input, number) {
        try {
            var value = input;
            if (value != null && !value.length < 1) {
                return value.split("-")[number];
            } else {
                return "";
            }
        } catch (e) {

        }
    }
    function js_searchbox_P() {

        var MaNCC = fomart_split($("#NhaCCFull").val(), 0);
        var Mahang = fomart_split($("#MaHangPop").val(), 0);
        var CuaHang = $("#CuaHang").val();
        var CuaHang = $("#CuaHang").val();
        var TinhThanh = $("#TinhThanh").val();
        var Mien = $("#Mien").val();
        js_Loading(true, 1);
        $.ajax({
            url: '/ManageSales/GetList_MoRongTrungBayHH',//QuanLyTrungBayHH_gateway_MoRongKD
            type: 'POST',
            data: JSON.stringify({ MaNCC: MaNCC, MaHang: Mahang, CuaHang: CuaHang, TinhThanh: TinhThanh, Mien: Mien }),
            contentType: 'application/json, charset=utf-8',
            beforeSend: function () {
                js_Loading(true, 1);
            },
            success: function (data) {
                $("#div-content_MRKD").find('tbody').empty();
                $('#div-content_MRKD').html(data);
                js_ReloadTable_MRKD();
                js_Loading(false, 1);
            },
            error: function () {
                js_Loading(false, 1);
            }
        });
    }
    function js_showpopup(Mahang, MaNCC) {

        Show_Filter(MaNCC, Mahang);
        js_searchbox_P();
        $('#popupMoRongKD').modal();
    }
    function js_ReloadTable_MRKD() {
        imgrecall();
        $('#tbl_Item_MRKD').DataTable({
            lengthMenu: [[100000, 50, 70, 100, -1], [100000, 50, 70, 100]],
            //"order": [[0, "desc"]],
            columnDefs: [
                { 'targets': 3, 'orderable': false },
                { 'targets': 4, 'orderable': false },
                { 'targets': 5, 'orderable': false },
            ],
            "searching": false,
            fixedHeader: false,
            //fixedHeader: {
            //    headerOffset: 185
            //},
            autoWidth: false,
            "language": {
                "emptyTable": "Không có dữ liệu !"
            }
        });
    }
    function Show_Filter(MaNCC, Mahang) {
        $('#CuaHang').empty();
        if (MaNCC.length > 0) {
            $.ajax({
                url: '/ManageSales/GateWay_TrungBayNganhHang_CuaHang',
                type: 'POST',
                data: JSON.stringify({ MaNCC: MaNCC, Mahang: Mahang }),
                contentType: 'application/json, charset=utf-8',
                beforeSend: function () {
                    js_Loading(true, 1);
                },
                success: function (data) {
                    var row = JSON.parse(data);
                    if (row != null) {
                        $('#CuaHang').empty();
                        $('#CuaHang').append('<option value="">-- Chọn cửa hàng --</option>');
                        for (i in row) {
                            $('#CuaHang').append('<option value="' + row[i].Code + '">' + row[i].Code + ' - ' + row[i].Name + '</option>');
                        }
                    }
                    $('#CuaHang').selectpicker('refresh');
                    js_Loading(false, 1);
                },
                error: function () {
                    js_Loading(false, 1);
                }
            });


            $('#Mien').empty();
            $.ajax({
                url: '/ManageSales/GateWay_TrungBayNganhHang_Mien',
                type: 'POST',
                data: JSON.stringify({ MaNCC: MaNCC, Mahang: Mahang }),
                contentType: 'application/json, charset=utf-8',
                beforeSend: function () {
                    js_Loading(true, 1);
                },
                success: function (data) {
                    var row = JSON.parse(data);
                    if (row != null) {
                        $('#Mien').empty();
                        $('#Mien').append('<option value="">-- Chọn vùng miền --</option>');
                        for (i in row) {
                            $('#Mien').append('<option value="' + row[i].Name + '">' + row[i].Code + ' - ' + row[i].Name + '</option>');
                        }
                    }
                    $('#Mien').selectpicker('refresh');
                    js_Loading(false, 1);
                },
                error: function () {
                    js_Loading(false, 1);
                }
            });

            $('#TinhThanh').empty();
            $.ajax({
                url: '/ManageSales/GateWay_TrungBayNganhHang_TinhThanh',
                type: 'POST',
                data: JSON.stringify({ MaNCC: MaNCC, Mahang: Mahang }),
                contentType: 'application/json, charset=utf-8',
                beforeSend: function () {
                    js_Loading(true, 1);
                },
                success: function (data) {
                    var row = JSON.parse(data);
                    if (row != null) {
                        $('#TinhThanh').empty();
                        $('#TinhThanh').append('<option value="">-- Chọn tỉnh thành --</option>');
                        for (i in row) {
                            $('#TinhThanh').append('<option value="' + row[i].Code + '">' + row[i].Code + ' - ' + row[i].Name + '</option>');
                        }
                    }
                    $('#TinhThanh').selectpicker('refresh');
                    js_Loading(false, 1);
                },
                error: function () {
                    js_Loading(false, 1);
                }
            });
        }
    }

    function js_saveCheck() {

        var cboxes = document.getElementsByName('checkMR[]');
        var arrCheckbox = [];
        var i = 0;
        for (var i = 0; i < cboxes.length; i++) {
            if (cboxes[i].checked) {
                var po = {};
                // alert(cboxes[i].id);
                var r = cboxes[i].id.replaceAll("Row_", "");

              //  console.log(r + '**' + cboxes[i].id);
                po.MaNCC = fomart_split($("#NhaCCFull").val(), 0);
                po.TenNCC = fomart_split($("#NhaCCFull").val(), 1);

                po.MaHang = fomart_split($("#MaHangPop").val(), 0);
                po.TenHang = fomart_split($("#MaHangPop").val(), 1);
                po.MaCuaHang = $('#Row' + cboxes[i].id + ' #MaCH' + r).html();
                po.TenCuaHang = $('#Row' + cboxes[i].id + ' #TenCH' + r).html();
                po.SoMatTB = $('#Row' + cboxes[i].id + ' #SoMatTB' + r).html();
                po.Stockday = $('#Row' + cboxes[i].id + ' #Stockday' + r).html();
                po.SoMatTBMoi = $('#Row' + cboxes[i].id + ' #SoMatTBMoi').val();
                po.TuNgay = $('#Row' + cboxes[i].id + ' #TuNgay').val();
                po.DenNgay = $('#Row' + cboxes[i].id + ' #DenNgay').val();
                po.ChiPhi = $('#Row' + cboxes[i].id + ' #ChiPhi' + r).html();

                po.VungMien = $('#Row' + cboxes[i].id + ' #Mien' + r).html();
                po.TinhThanh = $('#Row' + cboxes[i].id + ' #MaTinhCode' + r).html();

                console.log(po);
                arrCheckbox.push(po);

                $('#Row_' + i + ' #TuNgay').css('border', '1px solid #d7d7d7');
                $('#Row_' + i + ' #DenNgay').css('border', '1px solid #d7d7d7');

                var NgayBatDau = $('#Row' + cboxes[i].id + ' #TuNgay').val();
                var NgayKetThuc = $('#Row' + cboxes[i].id + ' #DenNgay').val();
                if (NgayBatDau > NgayKetThuc) {
