    <script type="text/javascript">
        function intNumberParse(strId) {
            var n = $('#' + strId).val().replace(/[ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz!#$&%^*+?^${}()|[\]\\;:,\\.'"<>]/g, '');
            if (n.length > 0) {
                $('#' + strId).val(n.toLocaleString());
            } else {
                $('#' + strId).val('0');
            }
        }
        function floatNumberParse(strId) {
            var n = $('#' + strId).val().replace(/[ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz!#$&%^*+?^${}()|[\]\\;:\\.'`"<>]/g, '');
            if (n.replace(',', '').length > 0) {
                $('#' + strId).val(n.toLocaleString());
            } else {
                $('#' + strId).val('0');
            }
        }
        var chieucao = $(window).height();
        $('.content-wrapper ').css({ "min-height": chieucao });
    </script>

