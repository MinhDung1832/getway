            type: "POST",
            url: "/Vendor/registrationTaxExist",
            data: '{registrationTax:"' + $('#registrationTax').val() + '"}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (r) {

                if (r.code == '0') {

                    $.ajax({
                        type: "POST",
                        url: "/Vendor/addVendor",
                        data: '{info:' + JSON.stringify(info_obj) + ',cap:' + JSON.stringify(cap_obj) + ',con:' + JSON.stringify(con_obj) + '}',
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (r1) {
                            var sub = JSON.parse(r1);
                            if (sub["code"] == '1') {
                                window.location.href = '/Product/Create';
                            } else {
                                alert("Có lỗi xảy ra thử lại sau!");
                            }
                        }
                    });

                } else {
                    alert("Mã số thuế đa được đăng ký!");
                }
            }
        });
    }

    function ReII(strSelID) {

        $('#' + strSelID + '_txt').append('<input type="button" id="btn_' + strSelID + '_del_' + $('#' + strSelID + '').val() + '"  name="' + $('#' + strSelID + '').val() + '-' + $('#' + strSelID + ' option:selected').text() + '" value="' + $('#' + strSelID + ' option:selected').text() + '  &#10006;" onclick="AddSEl(\'' + strSelID + '\',\'' + $('#' + strSelID + '').val() + '\',\'' + $('#' + strSelID + ' option:selected').text() + '\')" class="btn-primary" style="float:left; margin:3px;border-radius: 20px; font-size: 12px !important;"/>')

        var x = document.getElementById(strSelID);
        x.remove(x.selectedIndex);
    }

    function AddSEl(strSelID, strId, strVal) {

        var x = document.getElementById(strSelID);
        var option = document.createElement("option");
        option.value = strId;
        option.text = strVal;
        x.add(option);

        sortSelectOptions('#' + strSelID);

        document.getElementById("btn_" + strSelID + "_del_" + strId).remove();
    }

    function sortSelectOptions(selectElement) {
        var options = $(selectElement + " option");

        options.sort(function (a, b) {
            if (a.text.toUpperCase() > b.text.toUpperCase()) return 1;
            else if (a.text.toUpperCase() < b.text.toUpperCase()) return -1;
            else return 0;
        });

        $(selectElement).empty().append(options);
        $(selectElement).val("");
    }

    function readeMulti(strSelID) {
        var str_rt = "";
        var c = document.getElementById(strSelID + "_txt").children;
        for (i = 0; i < c.length; i++) {
            if (i == 0) {
                console.log(c[i].name);

                str_rt += c[i].name;
            } else {
                console.log('|' + c[i].name);

                str_rt += '|' + c[i].name;
            }

        }

        return str_rt;
    }
</script>
