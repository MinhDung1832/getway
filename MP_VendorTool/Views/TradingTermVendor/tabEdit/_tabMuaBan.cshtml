                        <th scope="col" style="width: 13% ">Ghi chú</th>
                        <th scope="col" style="width: 7% ">Thêm</th>
                    </tr>
                </thead>
                <tbody>
                    @if (ViewBag.list_line_bonus != null)
                    {
                        var bonus_i = 0;
                        foreach (objLINEBONUS box in (List<objLINEBONUS>)ViewBag.list_line_bonus)
                        {
                            if (box.TypeTab == "1")
                            {
                                bonus_i++;
                                <tr id="row_muaban_thuong_@bonus_i">
                                    <td scope="row" style="text-align: center;">
                                        <select class="form-control valid" name="bonusType" id="bonusType_@bonus_i" aria-invalid="false">
                                            @if (ViewBag.list_typeBonus != null)
                                            {
                                                foreach (objCombox box2 in (List<objCombox>)ViewBag.list_typeBonus)
                                                {
                                                    <option @(box.BonusType == box2.Name ? "selected" : String.Empty) value="@box2.Name">@box2.Name</option>
                                                }
                                            }
                                        </select>
                                    </td>
                                    <td><input type="text" id="salesLevel" name="salesLevel" class="form-control OrderValue" value="@box.SalesLevel" /></td>
                                    <td><input type="text" id="salesLevelDen" name="salesLevelDen" class="form-control OrderValue" value="@box.SalesLevelDen" /></td>
                                    <td>
                                        <select class="form-control valid" name="salesConditions" id="salesConditions_@bonus_i" aria-invalid="false">
                                            @if (ViewBag.list_Thuong != null)
                                            {
                                                foreach (objCombox box2 in (List<objCombox>)ViewBag.list_Thuong)
                                                {
                                                    <option @(box.SalesConditions == box2.Name ? "selected" : String.Empty) value="@box2.Name">@box2.Name</option>
                                                }
                                            }
                                        </select>
                                    </td>
                                    <td>
                                        <select class="form-control valid" name="DKDSTinhThuong" id="DKDSTinhThuong_@bonus_i" aria-invalid="false">
                                            @if (ViewBag.list_Thuong != null)
                                            {
                                                foreach (objCombox box2 in (List<objCombox>)ViewBag.list_Thuong)
                                                {
                                                    <option @(box.DKDSTinhThuong == box2.Name ? "selected" : String.Empty) value="@box2.Name">@box2.Name</option>
                                                }
                                            }
                                        </select>
                                    </td>
                                    <td><input type="text" id="discountPercent" name="discountPercent" class="form-control" value="@box.DiscountPercent" /></td>
                                    <td><input type="text" id="DiscountAmountNoVAT" name="DiscountAmountNoVAT" class="form-control" value="@box.DiscountAmountNoVAT" /></td>
                                    <td style="text-align: center;"><input style=" background: none !important; border: none; " id="description" name="description" class="form-control valid" value="Truy thu thưởng nếu đạt doanh số lũy kế" title="Truy thu thưởng nếu đạt doanh số lũy kế" aria-invalid="false"></td>
                                    <td style="text-align:center;width: 115px;" class="btn_add"><a href="javascript:void(0);" onclick="deleteRowThuong(this);" class="btn_add"><i class="fa fa-trash" aria-hidden="true"></i></a></td>
                                </tr>
                            }
                        }
                    }
                </tbody>
            </table>

        </div>
    </div>

