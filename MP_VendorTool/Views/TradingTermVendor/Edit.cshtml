    function js_PriceAfterDiscount(r) {


        let discountPrice = $('#rowKyGui_' + r + ' input[name="discountPrice"]').val().trim();
        var priceSeparate = commaSeparateNumber(discountPrice);

        $('#discountPrice_' + r).val(priceSeparate);

        let percentDiscount = $('#rowKyGui_' + r + ' input[name="percentDiscount"]').val().trim();
        if (discountPrice > 0 && percentDiscount > 0) {
            var disPrice = discountPrice.replace(/\./g, '');
            var priceAfterDiscount = disPrice * (1 - percentDiscount / 100);
            $('#rowKyGui_' + r + ' #priceAfterDiscount_' + r).html(commaSeparateNumber(Math.round(priceAfterDiscount)));
            $('#rowKyGui_' + r + ' #priceAfterInput_' + r).val(commaSeparateNumber(Math.round(priceAfterDiscount)));
        }
        if (discountPrice > 0 && parseInt(percentDiscount) == 0) {
            $('#rowKyGui_' + r + ' #priceAfterDiscount_' + r).html(discountPrice);
            $('#rowKyGui_' + r + ' #priceAfterInput_' + r).val(discountPrice);
        }
        if (discountPrice > 0 && percentDiscount.length < 0) {
            $('#rowKyGui_' + r + ' #priceAfterDiscount_' + r).html(discountPrice);
            $('#rowKyGui_' + r + ' #priceAfterInput_' + r).val(discountPrice);
        }
        else if (parseInt(discountPrice) == 0) {
            $('#rowKyGui_' + r + ' #priceAfterDiscount_' + r).html('0');
            $('#rowKyGui_' + r + ' #priceAfterInput_' + r).val('0');
        }
    }

