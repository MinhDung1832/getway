    function js_PriceAfterDiscount(r) {

        let discountPrice = $('#row_' + r + ' input[name="discountPrice"]').val().trim();
        var priceSeparate = commaSeparateNumber(discountPrice);

        $('#discountPrice_'+r).val(priceSeparate);

        let percentDiscount = $('#row_' + r + ' input[name="percentDiscount"]').val().trim();
        if (discountPrice > 0 && percentDiscount > 0) {
            var disPrice = discountPrice.replace(/\./g, '');
            var priceAfterDiscount = disPrice * (1 - percentDiscount/100);
            $('#priceAfterDiscount_' + r).html(commaSeparateNumber(priceAfterDiscount));
            $('#priceAfterInput_' + r).val(priceAfterDiscount);
        }
    }

    function commaSeparateNumber(val) {
        while (/(\d+)(\d{3})/.test(val.toString())) {
            val = val.toString().replace(/(\d+)(\d{3})/, '$1' + '.' + '$2');
        }
        return val;
    }

