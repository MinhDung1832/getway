    function ApDungSanPhamMuaBan_HH() {
        debugger;
        var VendorNo = $('#VendorNo').val().trim();
        var MaSpSKU = $('#MaSpSKU').val().trim();
        var TenSpSKU = '';
        var RangeReview = $('#RangeReview').val().trim();
        var IDContractNo = $('#IDContractNo').val().trim();
        var arrVendorNo = VendorNo.split(" - ");
        var TypeS = $('#TypeS').val();
        var Type = '1';
        if (TypeS == 'HHOA') {
            Type = '1';
        }
        if (TypeS == 'KGUI') {
            Type = '2';
        }

        if (arrVendorNo[0].length > 0) {
            $.ajax({
                url: '/TradingTermVendor/SP_TRADTERM_ApDungSp_HHOA_TK_KGUI_Detail',
                type: 'POST',
                data: JSON.stringify({ vendorNo: arrVendorNo[0], IDContractNo: IDContractNo, Type: Type, MaSpSKU: MaSpSKU, TenSpSKU: TenSpSKU, RangeReview: RangeReview }),
                contentType: 'application/json, charset=utf-8',
                beforeSend: function () {
                    js_Loading(true, 1);
                },
                success: function (data) {
                    $("#TabMuaBan_SKU").find('tbody').empty();
                    $('#TabMuaBan_SKU').html(data);
                    //js_ReloadTable();
                    js_Loading(false, 1);
                },
                error: function () {
                    js_Loading(false, 1);
                }
            });
        }
    }

