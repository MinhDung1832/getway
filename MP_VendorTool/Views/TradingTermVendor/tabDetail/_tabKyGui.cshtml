<div class="box_tab_6">
    <div class="row">
        <div class="col-md-2">
            <h5>
                ĐỔI TRẢ HÀNG
            </h5>
        </div>
        <div class="col-md-10">

            <table class="table table-bordered" id="table_kygui_doitra">
                <thead>
                    <tr>
                        <th scope="col" style="width: 19%;">Áp dụng đổi/trả đối với hàng hoá</th>
                        <th scope="col" style="width: 8%;">Đổi/Trả</th>
                        <th scope="col" style="width:18%;">
                            Thời hạn đổi trả <br />(… ngày kể từ ngày nhận hàng)
                        </th>
                        <th scope="col" style="width: 15%;">
                            Thời hạn thông báo <br />(Báo trước .. ngày)
                        </th>
                        <th scope="col" style="width: 10%;">Phạt chậm đổi trả (%)</th>
                        <th scope="col" style="width: 10%;">Ghi chú</th>
                        <th scope="col" style="width:8%;">Xác nhận đổi/trả</th>
                    </tr>
                </thead>
                <tbody>

                    @if (ViewBag.list_CO_RE_GO != null)
                    {
                        foreach (objCONTENT_RETURNS_GOODS box in (List<objCONTENT_RETURNS_GOODS>)ViewBag.list_CO_RE_GO)
                        {
                            <tr>
                                <td scope="row">
                                    @box.contentApply
                                </td>
                                <td>
                                    @{
                                        var k = true;
                                        foreach (objApplyReturnProduct box2 in (List<objApplyReturnProduct>)ViewBag.list_app_reproduct)
                                        {
                                            if (box2.TypeTab == "2")
                                            {
                                                if (box.contentApply == box2.confirmChange)
                                                {
                                                    <div>@box2.Satus</div>
                                                    k = false;
                                                    break;
                                                }
                                            }
                                        }
                                        if (k)
                                        {
                                            <div>Clear stock</div>
                                        }
                                    }
                                </td>
                                <td style="color:orangered;">
                                    @{
                                        var r = true;
                                        foreach (objApplyReturnProduct box2 in (List<objApplyReturnProduct>)ViewBag.list_app_reproduct)
                                        {
                                            if (box2.TypeTab == "2")
                                            {
                                                if (box.contentApply == box2.confirmChange)
                                                {
                                                    @box2.refundTerm
                                                    r = false;
                                                    break;
                                                }
                                            }
                                        }
                                        if (r)
                                        {
                                            @box.refundTerm
                                        }
                                    }
                                </td>
                                <td style="color:orangered;">
                                    @{
                                        var cc = true;
                                        foreach (objApplyReturnProduct box2 in (List<objApplyReturnProduct>)ViewBag.list_app_reproduct)
                                        {
                                            if (box2.TypeTab == "2")
                                            {
                                                if (box.contentApply == box2.confirmChange)
                                                {
                                                    <div>@box2.BaoTruocNgay</div>
                                                    cc = false;
                                                    break;
                                                }
                                            }
                                        }
                                    }
                                </td>

                                <td>
                                    @{
                                        var p = true;
                                        foreach (objApplyReturnProduct box2 in (List<objApplyReturnProduct>)ViewBag.list_app_reproduct)
                                        {
                                            if (box2.TypeTab == "2")
                                            {
                                                if (box.contentApply == box2.confirmChange)
                                                {
                                                    @box2.Penalty
                                                    p = false;
                                                    break;
                                                }
                                            }
                                        }
                                        if (p)
                                        {
                                            @box.Penalty
                                        }
                                    }

                                </td>
                                <td style="text-align:center">
                                    @{
                                        var c = true;
                                        foreach (objApplyReturnProduct box2 in (List<objApplyReturnProduct>)ViewBag.list_app_reproduct)
                                        {
                                            if (box2.TypeTab == "2")
                                            {
                                                if (box.contentApply == box2.confirmChange)
                                                {
                                                    <div>@box2.Conten</div>
                                                    c = false;
                                                    break;
                                                }
                                            }
                                        }

                                    }
                                </td>

                                <td style="text-align:center">
                                    @{
                                        var n = true;
                                        foreach (objApplyReturnProduct box2 in (List<objApplyReturnProduct>)ViewBag.list_app_reproduct)
                                        {
                                            if (box2.TypeTab == "2")
                                            {
                                                if (box.contentApply == box2.confirmChange)
                                                {
                                                    <input type="checkbox" class="form-check-input" name="checkDoiTra" value="@box.contentApply" checked>
                                                    n = false;
                                                    break;
                                                }
                                            }
                                        }
                                        if (n)
                                        {
                                            <input type="checkbox" class="form-check-input" name="checkDoiTra" value="@box.contentApply">
                                        }
                                    }

                                </td>
                            </tr>

                        }
                    }

                </tbody>
            </table>
        </div>
    </div>
</div>

