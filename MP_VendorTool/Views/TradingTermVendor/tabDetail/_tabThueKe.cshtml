@using MP_VendorTool.Common
@using MP_VendorTool.Models.Order
@using MP_VendorTool.Models.TradingTermVendor
@{
    var list_line_bonus = (List<objLINEBONUS>)ViewBag.list_line_bonus;
    var list_mkt_bonus = (List<objMKTBONUS>)ViewBag.list_mkt_bonus;
    var list_cusBonus = (List<objCUSTOMERBONUS>)ViewBag.list_cusBonus;
    var list_brandBonus = (List<objBrandBonus>)ViewBag.list_brandBonus;
    var list_ChiPhiVanChuyen = (List<obj_TRADTERM_CHIPHIVANHANH>)ViewBag.list_ChiPhiVanChuyen;
}
<style>
